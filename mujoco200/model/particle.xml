    <include file="scene.xml"/>

    <option solver="CG" tolerance="1e-6"/>

    <size nconmax="3000" njmax="3000" nstack="50000000"/>

    <visual>
        <map stiffness="100"/>
    </visual>

    <worldbody>
        <composite type="particle" count="10 10 10" spacing="0.07" offset="0 0 1">
            <geom size=".02" rgba=".8 .2 .1 1"/>
        </composite>
    </worldbody>
</mujoco>
